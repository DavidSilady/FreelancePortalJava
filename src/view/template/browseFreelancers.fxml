<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="522.0" prefWidth="706.0" stylesheets="@master.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.BrowseFreelancersController">
   <children>
      <TableView fx:id="FreelancersTableView" layoutX="225.0" layoutY="81.0" prefHeight="404.0" prefWidth="465.0">
        <columns>
          <TableColumn fx:id="FreelancerTableColumn" prefWidth="337.0" text="Freelancer" />
          <TableColumn fx:id="AdditionalTableColumn" prefWidth="126.0" />
        </columns>
      </TableView>
      <Label fx:id="Label1" layoutX="83.0" layoutY="23.0" text="This table shows freelancers whose gigs are best reviewed" visible="false">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <JFXButton fx:id="BestRatingButton" layoutX="42.0" layoutY="224.0" onAction="#showBestRated" prefHeight="37.0" prefWidth="133.0" text="Best rating" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font></JFXButton>
      <JFXButton fx:id="MostLanguagesButton" layoutX="42.0" layoutY="321.0" onAction="#showMostLanguages" text="Most languges" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </JFXButton>
   </children>
</AnchorPane>
